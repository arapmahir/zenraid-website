{
    "$schema": "node_modules/wrangler/config-schema.json",
    "name": "zenraid",
    "main": "src/index.js",
    "compatibility_date": "2024-12-01",
    "assets": {
        "directory": "./"
    },
    "d1_databases": [
        {
            "binding": "DB",
            "database_name": "zenraid-subscribers",
            "database_id": "YOUR_DATABASE_ID_HERE"
        }
    ],
    "observability": {
        "enabled": true
    }
}
